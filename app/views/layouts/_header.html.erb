<header>
  <nav id="main-nav">
  	<h1><%= link_to logoHeader, welcome_path %></h1>
    <ul>
    	<li><%= link_to "Home", welcome_path %></li> |
    	<li><%= link_to "About", about_path %></li> |
    	<li><%= link_to "Contact", contact_path %></li> |
    	<li><%= link_to "Help", help_path %></li> |
    	<% if user_signed_in? %>
          <li class="dropdown">
              <li><%= link_to current_user.fname, feed_path(current_user) %></li> |
              <li><%= link_to 'Edit', edit_user_registration_path %></li> |
              <li><%= link_to 'Log out', destroy_user_session_path, method: :delete %></li> |
          </li>
        <% else %>
          <li><%= link_to 'Log In', new_user_session_path, :style =>'color:#FFFFFF;' %></li> |
          <li><%= link_to 'Sign Up', new_user_registration_path, :style =>'color:#FFFFFF;' %></li> |
        <% end %>
    
	</ul>
  </nav>
</header>

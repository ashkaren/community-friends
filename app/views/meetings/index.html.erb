<div class="row">
  <div class="col-lg-3">
    <%= render 'appwide/side_links' %>
  </div>
  <div class="col-lg-9 well">
    <h1>Current Events</h1>
    <br>
    <%= month_calendar events: @meetings do |date, meetings| %>
      <%= date.day %>

      <% meetings.each do |meeting| %>
        <div>
          <%= link_to meeting.name, meeting %>
        </div>
      <% end %>
    <% end %>
    <br>
  </div>
</div>

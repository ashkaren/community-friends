<div id="links" class="well">
  <h5>
    <%= fa_icon 'user' %>
    <%= link_to 'My Page', user_path(current_user) %>
  </h5>
  <h5>
    <%= fa_icon 'envelope' %>
    <%= link_to 'Messages', messages_path %>
  </h5>
  <h5>
    <%= fa_icon 'newspaper-o' %>
    <%= link_to 'Activity', welcome_path %>
  </h5>
  <% if current_user.lead? %>
    <h5>
      <%= fa_icon 'child' %>
      <%= link_to 'Pending', users_path(:action => "index", :approved => "false") %>
    </h5>
  <% end %>
  <h5>
    <%= fa_icon 'group' %>
    <%= link_to 'Groups', groups_path %>
  </h5>
  <h5>
    <%= fa_icon 'calendar-o' %>
    <%= link_to 'Events', new_event_path %>
  </h5>
  <h5>
    <%= fa_icon 'calendar' %>
    <%= link_to 'Calendar', meetings_path %>
  </h5>
  <h5>
    <%= fa_icon 'arrow-circle-up' %>
    <%= link_to 'Leads', users_path(:action => "index", :leads => "true") %>
  </h5>
  <h5>
    <%= fa_icon 'thumbs-up' %>
    <%= link_to 'Choose Lead', users_path(:action => "index", :vote => "true") %>
  </h5>

</div>
